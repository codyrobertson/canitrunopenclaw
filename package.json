{
  "name": "ciroc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest run",
    "db:push": "npx drizzle-kit push",
    "db:push:test": "npx drizzle-kit push --config drizzle.config.test.ts",
    "db:push:staging": "npx drizzle-kit push --config drizzle.config.staging.ts",
    "db:seed": "npx tsx scripts/seed-neon.ts",
    "db:seed:test": "ENV_FILE=.env.test NODE_ENV=test npx tsx scripts/seed-neon.ts",
    "db:seed:staging": "ENV_FILE=.env.staging npx tsx scripts/seed-neon.ts"
  },
  "dependencies": {
    "@neondatabase/auth": "^0.1.0-beta.21",
    "@neondatabase/serverless": "^1.0.2",
    "dotenv": "^17.3.1",
    "drizzle-kit": "^0.31.9",
    "drizzle-orm": "^0.45.1",
    "lucide-react": "^0.564.0",
    "next": "16.1.6",
    "posthog-js": "^1.347.2",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.4",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^4.0.18"
  },
  "ignoreScripts": [
    "sharp",
    "unrs-resolver"
